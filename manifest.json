{
  "manifest_version": 3,
  "name": "moonlock",
  "version": "1.1",
  "description": "Forces black background and gray text on most pages",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["moon.css"],
      "run_at": "document_start"
    }
  ]
}
